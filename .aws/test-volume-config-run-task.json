{
    "containerDefinitions": [
        {
            "name": "app",
            "image": "alpine:3.4",
            "memory": 16,
            "entryPoint": ["ping"],
            "command": [
                "-c",
                "4",
                "example.com"
            ],
            "cpu": 8,
            "portMappings": [
                {
                    "name": "app-5432-tcp",
                    "containerPort": 5432,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "TOKEN",
                    "value": "123"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "ebs1",
                    "containerPath": "/var/lib",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "family": "github-actions-deploy-task-def-integ-tests-run-task-volume-config",
    "taskRoleArn": "arn:aws:iam::413216369237:role/github-actions-setup-stack-TaskDefTaskRole-iR2g2ANhQCjc",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "ebs1",
            "configuredAtLaunch": true
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2",
        "FARGATE"
    ],
    "cpu": "128",
    "memory": "128",
    "enableFaultInjection": false,
    "tags": []
}
